            <pair e1="DS1.d16.s3.e1" e2="DS1.d16.s3.e2" id="DS1.d16.s3.i2" interaction="False" />
            <pair e1="DS1.d16.s3.e0" e2="DS1.d16.s3.e2" id="DS1.d16.s3.i3" interaction="False" />
        </sentence>
        <sentence id="DS1.d16.s4" origId="17925795-1048" text="Our study demonstrates for the first time that GSK-3 and HDAC are respective initial targets for lithium and VPA to activate  BDNF promoter IV, and that this BDNF induction involves a novel responsive region in promoter IV of the BDNF gene.">
