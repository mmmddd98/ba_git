            <entity id="DS1.d16.s3.e1" origId="444732" charOffset="71-85" type="compound" text="trichostatin A"/>
            <entity id="DS1.d16.s3.e2" origId="P56517,P56518,Q13547,Q94517" charOffset="111-116" type="protein" text="HDAC1"/>
            <entity id="DS1.d16.s3.e3" origId="O18752,O18753,O18755,O70183,O97759,P14082,P21237,P23363,P23560,P25429,P25430,P25431,P25432,Q02193,Q06225,Q3SAT7,Q4L0Y3,Q5IS78,Q6LCI5,Q7YRB4,Q90322,Q95106,Q9TST3" charOffset="148-152" type="protein" text="BDNF"/>
